#***********************多数据源配置*******************************#
#mysql数据库驱动(mariadb适用)
driverClassNameMysql=com.mysql.jdbc.jdbc2.optional.MysqlXADataSource

#数据库用户名
usernamemysqluc=root
#数据库用户密码
passwordmysqluc=123456
#数据库链接URL
urlmysqluc=jdbc:mysql://192.168.0.36:3306/uc?useUnicode=true&amp;characterEncoding=UTF-8


#******************连接池中其他属性设置*********************#
#Atomikos连接池的最小、最大连接数，默认全部1
minPoolSize = 5
maxPoolSize = 100

#Atomikos连接池中连接的最大空闲时间，单位为秒，默认为60秒
maxIdleTime = 120